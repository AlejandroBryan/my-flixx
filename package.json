{
  "name": "movie_api",
  "version": "1.0.0",
  "description": "The development of a movie API.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "app": "nodemon --exec babel-node ./src/index.js",
    "production": "babel ./src --out-dir dist --copy-files",
    "lint": "eslint -c .eslintrc --ext .js src",
    "fix": "eslint --fix --ext .js src ",
    "format": "prettier --config .prettierrc --write src/",
    "dev": "concurrently \"npm run app\" "
  },
  "keywords": [
    "node",
    "js",
    "javascript",
    "api",
    "mongodb",
    "rest-api",
    "server-side-rendering",
    "SSR"
  ],
  "author": "Alejandro Bryan",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^7.32.0",
    "eslint-config-plugin": "^1.0.11",
    "eslint-config-prettier": "^8.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1"
  }
}
